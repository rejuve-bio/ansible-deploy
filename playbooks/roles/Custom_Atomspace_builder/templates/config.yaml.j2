paths:
  hugegraph_loader: "{{ hugegraph_loader_path | default('/app/hugegraph-loader/apache-hugegraph-loader-incubating-1.5.0/bin/hugegraph-loader.sh') }}"
  output_dir: "{{ output_dir | default('./output') }}"
cors:
  allow_origins: {{ cors_allow_origins | default(["*"]) | to_nice_json }}
  allow_credentials: {{ cors_allow_credentials | default(true) | to_json }}
  allow_methods: {{ cors_allow_methods | default(["*"]) | to_nice_json }}
  allow_headers: {{ cors_allow_headers | default(["*"]) | to_nice_json }}
uploads:
  session_timeout: {{ upload_session_timeout | default(5) }}